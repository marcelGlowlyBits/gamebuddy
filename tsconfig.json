{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@typography/*": ["./components/typography/*"],
      "@interaction/*": ["./components/interaction/*"],
      "@types/*": ["./types/*"],
      "@queries/*": ["./queries/*"],
      "@features/*": ["./features/*"],
      "@hooks/*": ["./hooks/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
